msgctxt ""
"01010200.xhp\n"
"par_id3148798\n"
"help.text"
msgid ""
"<ahelp hid=\"cui/ui/optsavepage/docinfo\">Specifies that the "
"<emph>Properties</emph> dialog will appear every time you select the "
"<emph>Save As</emph> command.</ahelp>"
msgstr ""
