"LibreLogo.xhp\n"
"par_590\n"
"help.text"
msgid "PRINT “text”[2] ; print “x”<br/> PRINT “text”[1:3] ; print “ex”<br/>"
msgstr ""
"IMPRIMEIX “text”[2] ; imprimeix “x”<br/> IMPRIMEIX “text”[1:3] ; imprimeix "
"“ex”<br/>"

#. FKPEY
#: LibreLogo.xhp
