msgstr "Relatiu"

#. 6DBxp
#: 05090100.xhp
msgctxt ""
"05090100.xhp\n"
"par_id3151183\n"
"help.text"
msgid ""
"<ahelp hid=\"modules/swriter/ui/formattablepage/relwidth\">Displays the "
"width of the table as a percentage of the page width.</ahelp>"
msgstr ""
"<ahelp hid=\"modules/swriter/ui/formattablepage/relwidth\">Mostra l'amplada "
"de la taula com a percentatge de l'amplada de la pàgina.</ahelp>"

#. BWgLB
