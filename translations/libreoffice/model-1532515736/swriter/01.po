"help.text"
msgid ""
"<variable id=\"texttab\"><ahelp hid=\".uno:ConvertTableText\">Converts the "
"selected text into a table, or the selected table into "
"text.</ahelp></variable>"
msgstr ""
"<variable id=\"texttab\"><ahelp hid=\".uno:ConvertTableText\" visibility=\""
"visible\">Converteix el text seleccionat en una taula, o la taula "
"seleccionada en text.</ahelp></variable>"

