"<bookmark_value>sagnats;en el text</bookmark_value>     "
"<bookmark_value>paràgrafs; sagnats</bookmark_value>     "
"<bookmark_value>sagnats francesos en els paràgrafs</bookmark_value>     "
"<bookmark_value>sagnats drets en els paràgrafs</bookmark_value>     "
"<bookmark_value>línies de text; sagnats</bookmark_value>     "
"<bookmark_value>canviar;sagnats</bookmark_value>"

