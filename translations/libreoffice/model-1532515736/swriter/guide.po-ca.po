msgstr "Gir del text"

#. dq39t
#: text_rotate.xhp
msgctxt ""
"text_rotate.xhp\n"
"bm_id3155911\n"
"help.text"
msgid ""
"<bookmark_value>text; rotating</bookmark_value>  "
"<bookmark_value>rotating;text</bookmark_value>"
msgstr ""
