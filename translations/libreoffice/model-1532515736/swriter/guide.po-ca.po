"<variable id=\"using_numbering\"><link "
"href=\"text/swriter/guide/using_numbering.xhp\" name=\"Numbering and "
"Numbering Styles\">Numbering and Numbering Styles</link> </variable>"
msgstr ""
"<variable id=\"using_numbering\"><link "
